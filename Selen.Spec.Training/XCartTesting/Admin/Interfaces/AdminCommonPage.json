{
  "$schema":  "../../Common/PageSchema.json",
  "controls": [
    {
      "controlName": "Close Dialog",
      "controlType": "Button",
      "by": "CssSelector",
      "value": "button.ui-dialog-titlebar-close"
    },
    {
      "controlName": "Left Menu Item",
      "controlType": "Hyperlink",
      "by": "XPath",
      "value": "//*[@id='leftMenu']//a[.='{0}']"
    },
    {
      "controlName": "Left Sub Menu Item",
      "controlType": "Hyperlink",
      "by": "XPath",
      "value": "//*[@id='leftMenu']//a[.='{0}']/../following-sibling::div[1]//a[.='{1}']"
    },
    {
      "controlName": "Page Title",
      "controlType": "Span",
      "by": "XPath",
      "value": "//*[@id='page-title']//span"
    }
  ],
  "pageName": "Admin Common Page",
  "title": "",
  "url": ""
}
