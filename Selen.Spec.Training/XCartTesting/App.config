<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <specFlow>
    <unitTestProvider name="MSTest" />
    <!-- For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config -->
  </specFlow>
  <appSettings>
    <add key="urladmin" value="http://localhost:81/admin.php?target=login"/>
    <add key="urluser" value="http://localhost:81/"/>
    <add key="waitShortTime" value="10"/>
    <add key="waitMediumTime" value="20"/>
    <add key="waitLongTime" value="30"/>
    <add key="dateTimeFormat" value="d"/>
    <add key="adminUser" value="user@example.com"/>
    <add key="adminPassword" value="1234"/>
    <add key="driversPath" value=".\..\..\..\drivers"/>
    <add key="browser" value="Firefox"/>
  </appSettings>
 </configuration>